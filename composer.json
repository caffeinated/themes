{
    "name": "caffeinated/themes",
    "description": "Laravel 5 Themes",
    "keywords": ["themes", "laravel", "caffeinated", "blade"],
    "license": "MIT",
    "authors": [
        {
            "name": "Shea Lewis",
            "email": "shea.lewis89@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1.3",
        "caffeinated/composer-installers": "*",
        "illuminate/support": "5.8.*"
    },
    "autoload": {
        "files": [
            "helpers/themes.php"
        ],
        "psr-4": {
            "Caffeinated\\Themes\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Caffeinated\\Themes\\ThemesServiceProvider"
            ],
            "aliases": {
                "Theme": "Caffeinated\\Themes\\Facades\\Theme"
            }
        }
    }
}
