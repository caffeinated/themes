{
    "name": "caffeinated/themes",
    "description": "Laravel 5 Themes",
    "keywords": ["themes", "laravel", "caffeinated", "blade"],
    "license": "MIT",
    "authors": [
        {
            "name": "Shea Lewis",
            "email": "shea.lewis89@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "caffeinated/manifest": "1.0.*",
        "illuminate/support": "5.4.*|5.5.*|5.6.*|5.7.*",
        "illuminate/filesystem": "5.4.*|5.5.*|5.6.*|5.7.*",
        "illuminate/view": "5.4.*|5.5.*|5.6.*|5.7.*",
        "illuminate/config": "5.4.*|5.5.*|5.6.*|5.7.*"
    },
    "autoload": {
        "psr-4": {
            "Caffeinated\\Themes\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Caffeinated\\Themes\\ThemesServiceProvider"
            ],
            "aliases": {
                "Theme": "Caffeinated\\Themes\\Facades\\Theme"
            }
        }
    }
}
